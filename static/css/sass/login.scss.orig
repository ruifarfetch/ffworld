@import '_variables';
@import '_mixins';


[data-page="login"] .tab{
    padding: 10px;
    &.active{
    background-color: $color-white;
	}
}

#login-signIn, #login-register{
	background-color: $color-white;
	padding:20px;

	@media (max-width: $fablet) {
		padding:10px;
	}
}

.icon-box-checked, .icon-radio-checked {
 -webkit-box-shadow: 1px 1px 0px #cccccc inset;
 -moz-box-shadow: 1px 1px 0px #cccccc inset;
 box-shadow: 1px 1px 0px #cccccc inset;
 background-color:$color-offWhite;
}

